<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Template using EJS</title>
  </head>
  <body>
    <h1>Template</h1>

    <p><%= message %></p>

    <h1>inclusion</h1>
    <ul>
      <% list.forEach(item => { %>
      <h3><%- include('./include.ejs' , {i : item}) %></h3>
      <% }) %>
    </ul>
  </body>
</html>
